        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading) {
    if (isLoading || newValue == '') {
        return;
    }

    var startingVLan = g_form.getValue('startingVLAN');
    var endingVLan = newValue;

    if (endingVLan <= startingVLan) {
        alert('Ending VLan must be greater than Staring Vlan');
        return false;
    } else {
		var totalVLans = endingVLan - startingVLan;
        g_form.setValue('numberOfIPsInRange',totalVLans);
        g_form.setValue('numberOfVLansInRange', totalVLans);

    }

