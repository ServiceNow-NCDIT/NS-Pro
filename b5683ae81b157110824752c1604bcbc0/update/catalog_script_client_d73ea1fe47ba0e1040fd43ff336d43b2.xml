        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading) {
   if (isLoading || newValue == '') {
      return;
   }
    var ga = new GlideAjax('x_g_sonc_ns_pro.cmdb_ci_site_ajax');
    ga.addParam('sysparm_name', 'getServices');
    ga.addParam('sysparm_site_id', newValue);
    ga.getXMLAnswer(getServices);

    function getServices(response) {

		// alert('response = ' + response);

		var objResponse = JSON.parse(response);
		for (i = 0; i < objResponse.length + 1; i++) {
			
			// alert('adding ' + objResponse[i].sys_id + ' with title ' + objResponse[i].svc_title);

			g_form.addOption('wan_service_to_change', objResponse[i].sys_id, objResponse[i].svc_title);
		}
    }
   
}]]></script>
