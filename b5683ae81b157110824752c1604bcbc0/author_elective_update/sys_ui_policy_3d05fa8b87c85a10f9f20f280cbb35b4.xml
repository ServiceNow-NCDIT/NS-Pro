        <script_true><![CDATA[function onCondition() {
		g_form.setReadOnly('ip_address',true);
		g_form.setReadOnly('interface_subnet_mask',true);
		g_form.setReadOnly('pe_host_circuit_id',true);
		g_form.setReadOnly('remote_site_circuit_vlan',true);

	//if user is not admin then set fields to readonly
	var canEdit = false;
	if(g_user.hasRoles('admin')){
		canEdit = true;
	}
	if(!canEdit){
		g_form.setReadOnly('ip_address',true);
		g_form.setReadOnly('interface_subnet_mask',true);
		g_form.setReadOnly('pe_host_circuit_id',true);
		g_form.setReadOnly('remote_site_circuit_vlan',true);
		
	}
}]]></script_true>
