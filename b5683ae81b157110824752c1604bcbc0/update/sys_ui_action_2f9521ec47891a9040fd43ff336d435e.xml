        <script><![CDATA[function openSuggestedTelcos() {
	var sysid = g_form.getUniqueValue();
    var zip = g_scratchpad.taskInformation.siteZipCode;
    var serviceCategory = g_form.getValue('network_service_category');
    var parmFixedQuery = 'zipSTARTSWITH' + zip + '^service_category=guaranteed^telco_provider!=NULL^ORDERBYpriority';
    var telcoZipW = new GlideModal('show_list');
    telcoZipW.setTitle("Select Suggested Telco by Service Category and Zip");
    telcoZipW.setPreference('table', 'x_g_sonc_ns_pro_telco_selection_by_zip_list');
    telcoZipW.setPreference('sysparm_fixed_query', parmFixedQuery);
    telcoZipW.setPreference('focusTrap', true);
    telcoZipW.setWidth(2000);
    telcoZipW.setPreference('sysparm_view', 'x_g_sonc_ns_pro_telco_selection_by_zip');
    telcoZipW.render();


}]]></script>
