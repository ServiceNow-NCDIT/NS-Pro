        <name>Get Geo Location</name>
        <onclick/>
        <order>100</order>
        <script><![CDATA[action.setRedirectURL(current);
current.update();
if (!current.isActionAborted()) {
    // call a script include to perform the work
    gs.addInfoMessage('Executing order circuit');
    var nsw2util = new NcditNsw2Utils2();
    var cirOrdResp = nsw2util.orderCircuits(current);
	gs.addInfoMessage(JSON.stringify(nsw2util.orderCircuits(current)));
	
    if (cirOrdResp.status == 'OK') {
        gs.addInfoMessage("Circuit order status: nsw2Util_orderCircuits called, return OK.");
		gs.addInfoMessage("nsw2Util_orderCircuits msg: " + cirOrdResp.message);
		current.provisioning_state='5';
		current.update();
    } else {
        gs.addErrorMessage("nsw2 server side script to order circuits was called and returned " + cirOrdResp.status + " , " + cirOrdResp.message);
    }

}
// redirect to here
action.setRedirectURL(current);
]]></script>
