        <name>Populate Circuit Order Data</name>
        <onclick/>
        <order>290</order>
        <script><![CDATA[action.setRedirectURL(current);
current.update();
if (!current.isActionAborted()) {
    // call a script include to perform the work
    // Instantiate the nsw2 utilities
    var nsw2util = new NcditNsw2Utils2();
	var popCirDataResp = nsw2util.populateCircuitOrderData(current);
	if (popCirDataResp.status == 'OK') {
		gs.addInfoMessage("Circuit order data populated. Verify before ordering circuits.");
		gs.addInfoMessage("populateCircuitOrderData msg: " + popCirDataResp.message);
		current.provisioning_state='3';
		current.update();
	} else {
		gs.addErrorMessage("Populate Circuit Order Data was called and returned " + popCirDataResp.status + " , " + popCirDataResp.message);
	}
}
// redirect to here
action.setRedirectURL(current);]]></script>
