        <name>Schedule FS Install</name>
        <onclick/>
        <order>600</order>
        <script><![CDATA[action.setRedirectURL(current);
current.update();
if (!current.isActionAborted()) {
    // call a script include to perform the work
    // Instantiate the nsw2 utilities
    var nsw2util = new NcditNsw2Utils2();
    var cpeResp = nsw2util.reqCpeInstallByFS(current);
    if (cpeResp.status == 'OK') {
        gs.addInfoMessage(cpeResp.message);	
		current.provisioning_state='9';
		current.update();
    } else {
        gs.addErrorMessage("Server side script to install CPE was called and returned " + cpeResp.status + " , " + cpeResp.message);
    }

}
else {
	gs.addInfoMessage('Action aborted');
}
// redirect to here
action.setRedirectURL(current);]]></script>
