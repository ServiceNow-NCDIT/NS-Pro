        <script><![CDATA[function onSubmit() {
	
	var wan = 0;
	var lan = 0;
	var wlan = 0;
	var voip = 0;
	
	if (g_form.getValue('request_managed_wan') == 'true') wan++;
	if (g_form.getValue('request_managed_lan') == 'true') lan++;
	if (g_form.getValue('request_managed_wlan') == 'true') wlan++;
	if (g_form.getValue('request_managed_voip') == 'true') voip++;
	
	var total = wan + lan + wlan + voip;
	
	if (total == 0) {
		alert('You must select a service option to submit');
		return false;
	}
	
	return confirm('Submitting this request will initiate provisioning processes for network fulfillment. Please confirm your authorization to proceed.');
   
}]]></script>
