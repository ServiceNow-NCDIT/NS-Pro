        <name>Populate Telco Provider 3</name>
        <onclick/>
        <order>1300</order>
        <script><![CDATA[action.setRedirectURL(current);
// current.update();
if (!current.isActionAborted()) {
	//gs.addInfoMessage('getProvider called...');
	var telco = new telco();
	var response = JSON.parse(telco.getProvider(current, '3'));
	//gs.addInfoMessage(response.message);
	if (response.success) {
		gs.addInfoMessage(response.message);
		current.telco_provider = response.sys_id.toString();
		current.telco_order_contact = response.poc;
		current.telco_order_email = response.email;
	}
	else {
		gs.addErrorMessage(response.message);
		gs.addErrorMessage("No ternary telco provider returned. Contact engineering to explore alternatives");
		current.telco_provider = '';
		current.telco_order_contact = '';
		current.telco_order_email = '';
	}
	// gs.addInfoMessage('UI action complete');
	current.update();
}
// redirect to here
action.setRedirectURL(current);]]></script>
