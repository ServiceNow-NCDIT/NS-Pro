        <script><![CDATA[(function executeRule(current, previous /*null when async*/) {

	var taskInformation = {};
	taskInformation.site = current.getValue('site');
	var zip = (current.getElement('site.location.zip').toString()).substring(0,5);
	taskInformation.siteZipCode = zip;
	taskInformation.telco_provider = current.getValue('telco_provider');
	taskInformation.circuit = current.getValue('circuit');
	taskInformation.taskSysId = current.getValue('sys_id');
	taskInformation.taskParentSysId = current.getValue('telco_order');
	if(current.telco_order.request_ha == true)
	{
		taskInformation.requestedHA = true;
	} else {
		taskInformation.requestedHA = false;
	}
	


	g_scratchpad.taskInformation = taskInformation;

	
	

})(current, previous);]]></script>
