        <name>Un-Assign Ports</name>
        <onclick>openPortstoRemoveList()</onclick>
        <order>100</order>
        <script><![CDATA[function openPortstoRemoveList() {
	var selectedSite = g_form.getValue('name');
	var selectedSiteID = g_form.getValue('site_identifier');
	var sysid = g_form.getUniqueValue();
	var siteInformation = {};
	siteInformation.selectedSite = selectedSite;
	siteInformation.selectedSiteID = selectedSiteID;
	siteInformation.selectedSiteSysId = sysid;
	g_scratchpad.siteInformation = siteInformation;
	var viewQuery = 'site=' + sysid + "^install_statusIN1,4";
	var w = new GlideModal('show_list');
    w.setTitle('Select Ports to Un-Assign to ' + selectedSiteID + ' - '+ selectedSite);
    w.setPreference('table', 'x_g_sonc_ns_pro_port_list');
    w.setWidth(750);
    w.setPreference('sysparm_sys_id', sysid);
    w.setPreference('sysparm_view', 'selectavailableportsremove');
	//w.setPreference('sysparm_force_row_count',20);
    w.setPreference('sysparm_fixed_query', viewQuery);
	w.setPreference('onLoad',function(){g_list.setRowsPerPage(20);});
    w.setPreference('focusTrap', true);
    w.render();


}]]></script>
